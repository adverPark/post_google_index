# 티스토리 사이트맵 URL
# 예시: https://yourblog.tistory.com/sitemap.xml
SITEMAP_URL=https://yourblog.tistory.com/sitemap.xml

# Google Service Account JSON 파일 경로
# 예시: service-account.json (프로젝트 루트에 위치)
SERVICE_ACCOUNT_FILE=service-account.json

# API 요청 설정
MAX_RETRY=3
REQUEST_DELAY=1

# 일일 최대 인덱싱 요청 수 (Google API 제한)
DAILY_LIMIT=200
